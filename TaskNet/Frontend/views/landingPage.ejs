    <section class="stats">
        <h2 class="section-title" style="color: white; -webkit-text-fill-color: white;">Why Choose TaskNet?</h2>
        <div class="stats-grid">
            <div class="stat-item">
                <div class="stat-number">100%</div>
                <div class="stat-label">Cloud-Based</div>
            </div>
            <div class="stat-item">
                <div class="stat-number">24/7</div>
                <div class="stat-label">Access Anywhere</div>
            </div>
            <div class="stat-item">
                <div class="stat-number">Secure</div>
                <div class="stat-label">Data Protection</div>
